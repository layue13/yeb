<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.RequestLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.common.pojo.RequestLog">
        <id column="id" property="id"/>
        <result column="methodName" property="methodName"/>
        <result column="endTime" property="endTime"/>
        <result column="startTime" property="startTime"/>
        <result column="status" property="status"/>
        <result column="msg" property="msg"/>
        <result column="method" property="method"/>
        <result column="host" property="host"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , methodName, endTime, startTime, status, msg, method, host
    </sql>

</mapper>
